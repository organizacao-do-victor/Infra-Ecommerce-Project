#cd to terraform folder
cd terraform && \
terraform init && \
#Waits for confirmation, -auto-approve removes it
terraform apply && \
#save outputs to file
terraform output > ../output && \
#cds out
cd .. && \

#format ansible hosts and other files from output
python3 format.py && \

#ansible
cd ansible && \
ansible-playbook playbookLH.yml -i ansibleHost --private-key ../terraform/ecommerce-key